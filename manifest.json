{
  "name": "ENS Resolver",
  "description": "Attempts to resolve all Ethereum addresses found in the page",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_icon": {
      "16": "/images/ens-logo.png",
      "32": "/images/ens-logo.png",
      "48": "/images/ens-logo.png",
      "128": "/images/ens-logo.png"
    }
  },
  "icons": {
    "16": "/images/ens-logo.png",
    "32": "/images/ens-logo.png",
    "48": "/images/ens-logo.png",
    "128": "/images/ens-logo.png"
  }
}
